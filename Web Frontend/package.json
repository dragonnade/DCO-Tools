		"@types/diff-match-patch": "^1.0.36",
		"autoprefixer": "^10.4.20",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
